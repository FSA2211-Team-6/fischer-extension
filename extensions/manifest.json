{
  "manifest_version": 3,
  "version": "0.1",
  "name": "Fischer Fact Checker",
  "description": "Fischer is a simple fact checking ",
  "action": {
    "default_popup": "./dist/index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage", "contextMenus"]
}
